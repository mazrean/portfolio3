---
import Works from '@components/ecosystem/Works.astro'
import Presentation from '@components/ecosystem/Presentation.astro'
import Article from '@components/ecosystem/Article.astro'
import Layout from 'src/layouts/Layout.astro'
import works from '@yaml/works'
import presentation from '@yaml/presentation'
import article from '@yaml/article'
---

<Layout title="Achievements" location="/achievement">
  <Works worksList={works} />
  <Presentation presentationList={presentation} />
  <Article articleList={article.filter(v => !v.ignore)} />
</Layout>
