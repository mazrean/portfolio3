---
import Icon from "@components/atom/Icon.astro";

interface Props {
  class?: string;
  size: string;
  name: string;
}

const { class: className, size, name, ...rest } = Astro.props;
---

<Icon
  class={className}
  size={size}
  src={`/public/icons/${name}.svg`}
  alt="{name} icon"
  {...rest}
/>
